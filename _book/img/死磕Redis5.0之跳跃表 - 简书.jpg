    <span class="token keyword">if</span> <span class="token punctuation">(</span>zobj<span class="token operator">-</span><span class="token operator">&gt;</span><span class="token property">encoding</span> <span class="token operator">==</span> <span class="token constant">OBJ_ENCODING_ZIPLIST</span><span class="token punctuation">)</span> <span class="token punctuation">{</span>
        <span class="token punctuation">.</span><span class="token punctuation">.</span><span class="token punctuation">.</span>
    <span class="token punctuation">}</span> <span class="token keyword">else</span> <span class="token keyword">if</span> <span class="token punctuation">(</span>zobj<span class="token operator">-</span><span class="token operator">&gt;</span><span class="token property">encoding</span> <span class="token operator">==</span> <span class="token constant">OBJ_ENCODING_SKIPLIST</span><span class="token punctuation">)</span> <span class="token punctuation">{</span><span class="token comment">// 编码格式是跳跃表</span>
        zset <span class="token operator">*</span>zs <span class="token operator">=</span> zobj<span class="token operator">-</span><span class="token operator">&gt;</span><span class="token property">ptr</span><span class="token punctuation">;</span>
        dictEntry <span class="token operator">*</span>de<span class="token punctuation">;</span>
        double score<span class="token punctuation">;</span>
