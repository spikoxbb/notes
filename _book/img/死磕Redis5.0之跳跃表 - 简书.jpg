               <span class="token punctuation">(</span>x<span class="token operator">-</span><span class="token operator">&gt;</span><span class="token property">level</span><span class="token punctuation">[</span>i<span class="token punctuation">]</span><span class="token punctuation">.</span>forward<span class="token operator">-</span><span class="token operator">&gt;</span><span class="token property">score</span> <span class="token operator">&lt;</span> score <span class="token operator">||</span>
